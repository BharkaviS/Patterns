/*

Input:
5
Output:
1 6 10 13 15
2 7 11 14
3 8 12 
4 9 
5

*/
import java.util.*;

public class ZohoPat2
{
	public static void main(String[] args) {
		int n,i,j,k=1,diff=5,l;
		Scanner sc = new Scanner(System.in);
		n=sc.nextInt();
		for(i=n;i>0;i--){
		    diff = 5;
		    l=k;
		    System.out.print(k+" ");
		    for(j=1;j<i;j++){
		        l=l+diff;
		        System.out.print(l+" ");
		        diff--;
		    }
		    k++;
		    System.out.println(" ");
		}
	}
}
